{
  "version": 2,
  "builds": [
    {
      "src": "api_fastapi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "pythonRequirements": "requirements-vercel.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api_fastapi.py"
    }
  ],
  "regions": ["ams1"],
  "functions": {
    "api_fastapi.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
